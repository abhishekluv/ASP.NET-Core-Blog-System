@model Tag


<ol class="list-group list-group-numbered">
    @foreach(var blogPost in Model.Posts)
    {
        <li class="list-group-item d-flex justify-content-between align-items-start">
            <div class="ms-2 me-auto">
                <div class="fw-bold"><a title="@blogPost.Title" asp-action="Post" asp-controller="Blog" asp-route-year="@blogPost.DatePublished.Value.ToString("yyyy")" asp-route-month="@blogPost.DatePublished.Value.ToString("MM")" asp-route-date="@blogPost.DatePublished.Value.ToString("dd")" asp-route-slug="@blogPost.Slug">@blogPost.Title</a></div>
            </div>
        </li>
    }
</ol>




